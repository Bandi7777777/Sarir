{
  "root_dir": "..",
  "output_dir": "outputs_light/merged_parts",
  "assets_dir": "assets",
  "max_chars_per_part": 180000,
  "include_paths": [
    "packages/frontend/app",
    "packages/frontend/pages",
    "packages/frontend/src",
    "packages/frontend/components",
    "packages/frontend/lib",
    "packages/frontend/hooks",
    "packages/frontend/styles",
    "packages/frontend/public",
    "packages/backend/src",
    "packages/backend/prisma",
    "docs"
  ],
  "include_files_glob": [
    "package.json",
    "pnpm-lock.yaml",
    "yarn.lock",
    "package-lock.json",
    "tsconfig*.json",
    "next.config.*",
    "tailwind.config.*",
    "postcss.config.*",
    ".eslintrc*",
    ".prettierrc*",
    "README.md",
    "CHANGELOG.md"
  ],
  "include_extensions": [
    ".ts",
    ".tsx",
    ".js",
    ".jsx",
    ".mjs",
    ".cjs",
    ".py",
    ".json",
    ".html",
    ".css",
    ".scss",
    ".md",
    ".txt",
    ".yml",
    ".yaml",
    ".env.example"
  ],
  "include_any_utf8_text": false,
  "include_vscode_settings": false,
  "excluded_dirs": [
    ".git",
    "node_modules",
    ".pnpm-store",
    ".yarn",
    ".yarn/cache",
    ".yarn/unplugged",
    ".next",
    "out",
    "build",
    "dist",
    ".turbo",
    ".cache",
    ".rollup.cache",
    ".parcel-cache",
    ".sass-cache",
    ".eslintcache",
    ".swc",
    ".webpack",
    ".vite",
    ".vercel",
    ".netlify",
    "firebase",
    "coverage",
    "cypress",
    "playwright-report",
    "logs",
    "tmp",
    "temp",
    "__tests__",
    "__mocks__",
    ".storybook"
  ],
  "excluded_globs": [
    "**/*.log",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/desktop.ini",
    "**/.env*",
    "**/*.spec.*",
    "**/*.test.*",
    "**/*.stories.*",
    "**/*.snap",
    "**/mocks/**",
    "**/mock/**",
    "**/fixtures/**",
    "**/examples/**",
    "**/example/**",
    "packages/**/public/uploads/**",
    "packages/**/public/media/**",
    "packages/**/public/images/raw/**"
  ],
  "image_keep_under_kb": 200,
  "image_extensions": [
    ".png",
    ".jpg",
    ".jpeg",
    ".gif",
    ".webp",
    ".svg"
  ],
  "redactions": [
    "[A-Za-z0-9_\\-]{24,}\\.[A-Za-z0-9_\\-]{6,}\\.[A-Za-z0-9_\\-]{15,}",
    "\\b[A-Za-z0-9]{32,}\\b",
    "\\b(eyJhbGciOi|ya29\\.)[A-Za-z0-9\\-\\._]+\\b"
  ]
}