{
  "root_dir": "..",
  "output_dir": "outputs/merged_parts",
  "assets_dir": "assets",
  "max_chars_per_part": 150000,
  "include_extensions": [
    ".ts",
    ".tsx",
    ".js",
    ".jsx",
    ".mjs",
    ".cjs",
    ".py",
    ".json",
    ".html",
    ".css",
    ".scss",
    ".md",
    ".txt",
    ".csv",
    ".xml",
    ".yml",
    ".yaml",
    ".env.example"
  ],
  "include_any_utf8_text": true,
  "include_vscode_settings": false,
  "excluded_dirs": [
    ".git",
    "node_modules",
    ".pnpm-store",
    ".yarn",
    ".yarn/cache",
    ".yarn/unplugged",
    ".next",
    "out",
    "build",
    "dist",
    ".turbo",
    ".cache",
    ".rollup.cache",
    ".parcel-cache",
    ".sass-cache",
    ".eslintcache",
    ".swc",
    ".webpack",
    ".vite",
    ".vercel",
    ".netlify",
    "firebase",
    "coverage",
    "cypress",
    "playwright-report",
    "logs",
    "tmp",
    "temp"
  ],
  "excluded_globs": [
    "**/*.log",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/desktop.ini",
    "**/.env*",
    "public/uploads/**",
    "public/media/**",
    "public/images/raw/**",
    "packages/**/public/uploads/**",
    "packages/**/public/media/**",
    "packages/**/public/images/raw/**"
  ],
  "image_keep_under_kb": 700,
  "image_extensions": [
    ".png",
    ".jpg",
    ".jpeg",
    ".gif",
    ".webp",
    ".svg"
  ],
  "redactions": [
    "[A-Za-z0-9_\\-]{24,}\\.[A-Za-z0-9_\\-]{6,}\\.[A-Za-z0-9_\\-]{15,}",
    "\\b[A-Za-z0-9]{32,}\\b",
    "\\b(eyJhbGciOi|ya29\\.)[A-Za-z0-9\\-\\._]+\\b"
  ]
}